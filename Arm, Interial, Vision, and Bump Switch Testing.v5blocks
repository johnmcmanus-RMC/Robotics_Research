train.driveFor(forward, 2500.0, mm, true);\n  wait(2.0, seconds);\n  Drivetrain.turnFor(left, 90.0, degrees, true);\n  Drivetrain.stop();\n  wait(2.0, seconds);\n  Drivetrain.turnFor(left, 90.0, degrees, true);\n  // End of Explore Phase\n  Drivetrain.stop();\n  message1.broadcast();\n  return 0;\n}\n\n// \"when BumperRight pressed\" hat block\nvoid onevent_BumperRight_pressed_0() {\n  Brain.Screen.print(\"Right Bumper Pressed\");\n  wait(2.0, seconds);\n  message1.broadcast();\n}\n\n// \"when BumperLeft pressed\" hat block\nvoid onevent_BumperLeft_pressed_0() {\n  Brain.Screen.print(\"Left Bumper Pressed\");\n  wait(2.0, seconds);\n  message1.broadcast();\n}\n\n// \"when I receive message1\" hat block\nvoid onevent_message1_0() {\n  Brain.Screen.print(\"End of Task Actions\");\n  ArmMotor.spinToPosition(0.0, degrees, true);\n  wait(2.0, seconds);\n  vexSystemExitRequest();\n}\n\n\nint main() {\n  // Calibrate the Drivetrain\n  calibrateDrivetrain();\n\n  // register event handlers\n  BumperRight.pressed(onevent_BumperRight_pressed_0);\n  BumperLeft.pressed(onevent_BumperLeft_pressed_0);\n  message1(onevent_message1_0);\n\n  wait(15, msec);\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  whenStarted1();\n}","target":"Physical"}